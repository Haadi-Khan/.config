{
  "adapters": {
    "CodeLLDB": {
      "command": [
        "${gadgetDir}/CodeLLDB/adapter/codelldb",
        "--port",
        "${unusedLocalPort}"
      ],
      "configuration": {
        "args": [],
        "cargo": {},
        "cwd": "${workspaceRoot}",
        "env": {},
        "name": "lldb",
        "terminal": "integrated",
        "type": "lldb"
      },
      "name": "CodeLLDB",
      "port": "${unusedLocalPort}",
      "type": "CodeLLDB"
    },
    "multi-session": {
      "host": "${host}",
      "port": "${port}"
    },
    "vscode-cpptools": {
      "attach": {
        "pidProperty": "processId",
        "pidSelect": "ask"
      },
      "command": [
        "${gadgetDir}/vscode-cpptools/debugAdapters/bin/OpenDebugAD7"
      ],
      "configuration": {
        "args": [],
        "cwd": "${workspaceRoot}",
        "environment": [],
        "type": "cppdbg"
      },
      "name": "cppdbg"
    }
  }
}